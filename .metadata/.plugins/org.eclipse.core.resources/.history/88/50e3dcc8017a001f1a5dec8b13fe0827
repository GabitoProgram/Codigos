package CLASE_1;
import java.util.*;

public class Main {

	public static void main(String[] args) {
		Scanner lee = new Scanner(System.in);
		while(lee.hasNext()) {
			int n = lee.nextInt();
			
		}
	}
	public static int dpa(int n) {
		int count=0;
		for(int i=1;i<n;i++) {
			if(n%i ==0) {
				count=count+i;
			}
		}
		return count;
	}
}

import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner lee =  new Scanner(System.in);  
		int n=2;
		int aux= primo(n);
		if(aux==2) {
			System.out.println("es primo");
		}
		else
			System.out.println("no es primo");
	}
	
	public static int primo(int n) {
		int cont=0;
		for(int i=1;i<=n;i++) {
			if(n%i==0)
				cont=cont+1;
		}
		return cont;
	}

	
	
	
}
